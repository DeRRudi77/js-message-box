{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "8",
          "chrome": 16,
          "edge": 12,
          "firefox": 11,
          "ie": 10,
          "safari": 7
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "transform-es2015-modules-commonjs"
  ]
}
